    "homepage": "https://github.com/madtechservices/theme-madmin",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "ThemeMadmin"
    ],
    "require": {
        "backpack/crud": "^6.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Madtechservices\\ThemeMadmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Madtechservices\\ThemeMadmin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "laravel": {
        "providers": [
            "Madtechservices\\ThemeMadmin\\AddonServiceProvider"
        ]
    }
}

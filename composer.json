{
    "name": "madtechservices/theme-madmin",
    "description": "MADmin - Tailwind based UI for Backpack v6.",
    "version": "0.0.18",
    "type": "laravel-backpack-addon",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Horsley",
            "email": "matt@madtechservices.com.au",
            "homepage": "https://madtechservices.com.au"
        },
        {
            "name": "MAD Tech Services",
            "email": "support@madtechservices.com.au",
            "homepage": "https://madtechservices.com.au"
        }
    ],
    "homepage": "https://github.com/madtechservices/theme-madmin",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "ThemeMadmin"
    ],
    "require": {
        "backpack/crud": "^6.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\ThemeTabler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\ThemeTabler\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Backpack\\ThemeMadmin\\AddonServiceProvider"
            ]
        }
    }
}
